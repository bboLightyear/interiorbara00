<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tech.ibara.oh.dao.OHInterfaceDao">
	
	<!-- OHPhotoView.jsp -->
	<select id="ohPhotoView" resultType="com.tech.ibara.oh.dto.OHPhotoBoard">
		SELECT
			* 
		FROM 
			OH_PHOTO_BOARD
	</select>
	<!-- OHPhotoWriteExecute -->
	<insert id="ohPhotoWriteExecute">
		INSERT INTO 
		    OH_PHOTO_BOARD
		VALUES (
			OH_PHOTO_BOARD_SEQ.NEXTVAL, NULL, #{param1}, #{param2}, SYSDATE, 
		    0, 0, 0, 0, 0, 
		    #{param3}, #{param4}, #{param5}, #{param6}, #{param7}
		)
	</insert>
	<select id="getRecentPb_no" resultType="Integer">
		SELECT 
			MAX(PB_NO) 
		FROM 
			OH_PHOTO_BOARD
	</select>
</mapper>